{{extend 'layout.html'}}
<table>
<tr>
<td><h4>Agenda</h4></td>
<td><h4>Room</h4></td>
<td><h4>Start Date</h4></td>
<td><h4>Start Time</h4></td>
<td><h4>Close Date</h4></td>
<td><h4>Close Time</h4></td>
    <td><h4>
        Attachment
        </h4></td>
</tr>
{{for meet in meets:}}
<tr>
<td>{{=meet.agenda}}</td>
<td>{{=meet.room}}</td>
<td>{{=meet.date_in}}</td>
<td>{{=meet.time_in}}</td>
<td>{{=meet.date_out}}</td>
<td>{{=meet.time_out}}</td>
    {{if meet.file:}}
    <td><a href="{{=URL(r=request,f='download',args=meet.file)}}"><img src="{{=URL(r=request,c='static',f='images.jpg')}}></a></td>
{{else:}}
    {{pass}}
</tr>
{{pass}}
</table>
